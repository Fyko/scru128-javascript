{
  "name": "scru128",
  "version": "0.2.4-rc.0",
  "description": "SCRU128: Sortable, Clock and Random number-based Unique identifier",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": "./esm.mjs",
    "require": "./dist/index.js"
  },
  "files": [
    "dist",
    "esm.mjs"
  ],
  "sideEffects": false,
  "browser": {
    "crypto": "./dist/crypto_dummy.js"
  },
  "scripts": {
    "build": "tsc && webpack ./test/*.mjs -o ./test/bundle --mode production",
    "doc": "typedoc ./src/index.ts --gitRevision \"v$npm_package_version\"",
    "prebuild": "rm -rf ./dist ./test/bundle",
    "predoc": "rm -rf ./docs",
    "prepare": "npm run build && npm run doc && npm run test",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scru128/javascript.git"
  },
  "keywords": [
    "identifier",
    "uuid",
    "guid",
    "ulid",
    "ksuid"
  ],
  "author": "LiosK <contact@mail.liosk.net>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/scru128/javascript/issues"
  },
  "homepage": "https://github.com/scru128/javascript#readme",
  "devDependencies": {
    "mocha": "^9.1.3",
    "typedoc": "^0.22.6",
    "typescript": "^4.4.4",
    "webpack": "^5.59.1",
    "webpack-cli": "^4.9.1"
  }
}
