{
  "name": "scru128",
  "version": "2.3.1",
  "description": "SCRU128: Sortable, Clock and Random number-based Unique identifier",
  "type": "module",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "node": {
      "require": "./dist/cjs/node.cjs",
      "default": "./dist/node.js"
    },
    "require": "./dist/cjs/index.cjs",
    "default": "./dist/index.js"
  },
  "files": [
    "CHANGELOG.md",
    "dist"
  ],
  "sideEffects": [
    "./dist/node.js",
    "./dist/cjs/node.cjs"
  ],
  "browser": {
    "crypto": false
  },
  "scripts": {
    "build": "tsc && webpack ./test/*.mjs -o ./test/bundle --mode production",
    "doc": "typedoc ./src/index.ts --gitRevision \"v$npm_package_version\"",
    "postbuild": "rm -f ./dist/cjs/*.d.cts",
    "prebuild": "rm -rf ./dist ./test/bundle",
    "predoc": "rm -rf ./docs",
    "prepare": "npm run build && npm run doc && npm run test",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scru128/javascript.git"
  },
  "keywords": [
    "identifier",
    "uuid",
    "guid",
    "ulid",
    "ksuid"
  ],
  "author": "LiosK <contact@mail.liosk.net>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/scru128/javascript/issues"
  },
  "homepage": "https://github.com/scru128/javascript#readme",
  "devDependencies": {
    "mocha": "^10.2.0",
    "typedoc": "^0.23.25",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
