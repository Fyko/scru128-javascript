{
  "include": ["./src/cjs"],
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "moduleResolution": "node",
    "typeRoots": [],
    // "declaration": true,
    "outDir": "./dist/cjs",
    "stripInternal": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
